<% @propertylistings.last(20).each do |house| %>
    $('#property-listing tr:last').before(" <%= escape_javascript(render :partial => 'property_listings/property_listing', :locals => { :property_listing => house, :url => @url }  ) %> " );
<% end %>
$(".load-more")[0].innerHTML = "Load more...<br>Results for <%= @location %>, showing <%= @propertylistings.count.to_s %> of <%= @total_number.to_s %> properties";
$('.page-header h1').text("<%= @propertylistings.count.to_s+" of "+@total_number.to_s+" matches"%>")
history.replaceState(null, document.title, "<%= listing_url(@location)+"?page="+(@propertylistings.count/20).to_s %>");
